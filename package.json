{
  "name": "n8n-nodes-passwork",
  "version": "0.3.1",
  "description": "Passwork integration for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Boris Gorbylev",
    "email": "ekho@ekho.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekho/n8n-nodes-passwork.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "echo 'Nothing to test'",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PassworkApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Passwork/Passwork.node.js"
    ]
  },
  "dependencies": {
      "js-sha256": "^0.9.0",
      "abab": "^2.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/parser": "~6.7.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n": "^1 >=1.4",
    "n8n-core": "^1 >=1.4",
    "n8n-workflow": "^1 >=1.4",
    "prettier": "^3.0.3",
    "typescript": "~5.2.2"
  }
}
