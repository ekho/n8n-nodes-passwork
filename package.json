{
  "name": "n8n-nodes-passwork",
  "version": "0.1.0",
  "description": "Passwork integration for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekho/n8n-nodes-passwork.git"
  },
  "main": "index.js",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "postinstall": "pnpm build",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PassworkApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Passwork/Passwork.node.js"
    ]
  },
  "dependencies": {
    "passwork-js": "^1.2.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/parser": "~5.45.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n": "^1.4.1",
    "n8n-core": "^1.1.1",
    "n8n-workflow": "^1.1.1",
    "prettier": "^2.8.8",
    "typescript": "~4.8.4"
  }
}
